<p style="color: green"><%= notice %></p>

<h1>Property_value</h1>

<div id="property_value">
  <% @property_values.each do |property_value| %>
    <%= render property_value %>
    <p>
      <%= link_to "Show this value", property_value %>
    </p>
  <% end %>
</div>

<%= link_to "New value", new_property_value_path %>
