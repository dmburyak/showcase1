= search_form_for @q, url: search_phones_path,
                      html: { method: :post } do |f|

  = f.label property_name(41)
  = f.collection_check_boxes :property_values_property_data_in, PropertyValue.where(property_id: 41),
                              :property_data, :property_data

  = f.label :name_or_description_cont
  = f.search_field :name_or_description_cont

  = f.submit